#include <iostream>
using namespace std;
 
void print(int i, int j) {
  int temp;
  temp = i;
  i=j;
  j=temp;
  cout << " Here are ints " << i << " | "<< j << endl;
}

void print(float  f1, float f2) {
  float f3;
  f3 = f1;
  f1=f2;
  f2=f3;
  cout << " Here are floats " << f1 << endl;
}

int main() {
  print(6, 9);
  print(6.9, 4.2);

  return 0;
}
